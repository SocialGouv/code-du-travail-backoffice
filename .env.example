########################################
# Custom

API_URI=http://localhost:3200
DATA_FILLER_PATH=/admin/data-filler
NODE_ENV=development
WEB_PORT=3100

########################################
# CI, development & test variables

# Migrations, seeds and development (next.js server):
DB_PUBLIC_URI=postgresql://test_db_user:test_db_password@localhost:5433/test_db

# Production script & E2E tests:
TEST_WEB_URI=http://localhost:3100

########################################
# Kinto

KINTO_BUCKET=datasets
KINTO_PORT=8889
KINTO_URI=http://127.0.0.1:8889

########################################
# PostgreSQL

POSTGRES_DB=test_db
POSTGRES_KINTO_DB=test_kinto_db
POSTGRES_PASSWORD=test_db_password
POSTGRES_PORT=5433
POSTGRES_USER=test_db_user

########################################
# PostgREST

# In production, this role should not be the same as the one used for the
# connection:
PGRST_DB_ANON_ROLE=anonymous
PGRST_DB_SCHEMA=api
# It must be at least 32 characters long:
# https://github.com/PostgREST/postgrest/issues/977#issuecomment-329917367
PGRST_JWT_SECRET=a_test_only_postgrest_jwt_secret
PGRST_PORT=3200
