{
  "name": "@socialgouv/cdtn-backoffice",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "description": "Applications d'administration des données du code du travail numérique.",
  "private": true,
  "scripts": {
    "build": "lerna run --scope \"@socialgouv/cdtn-contrib\" build",
    "ci:test:e2e": "NODE_ENV=test ./scripts/prod/start.sh && yarn test:e2e",
    "db:backup": "node ./scripts/db/backup.js",
    "db:migrate": "knex migrate:latest",
    "db:migrate:make": "node ./scripts/db/generateMigration.js",
    "db:init": "docker-compose exec -T db ./scripts/create_kinto_database.sh",
    "db:seed": "knex seed:run && node ./packages/data-filler/fixture.js",
    "dev": "lerna run --scope \"@socialgouv/cdtn-contrib\" dev --parallel",
    "docker:web": "cross-env NODE_ENV=production docker-compose up --build -d web",
    "postinstall": "lerna link",
    "setup": "cpy --rename=\".env\" ./.env.example .",
    "start": "lerna run --scope \"@socialgouv/cdtn-contrib\" start",
    "start:prod": "NODE_ENV=production ./scripts/prod/start.sh",
    "test": "lerna run test",
    "test:e2e": "cucumber-js",
    "test:lint": "lerna run test:lint",
    "test:unit": "lerna run test:unit",
    "test:update": "lerna run test:update",
    "test:watch": "lerna run test:watch --parallel"
  },
  "dependencies": {
    "colors": "1.4.0",
    "dotenv": "8.2.0",
    "knex": "0.20.7",
    "lerna": "3.20.2",
    "moment": "2.24.0",
    "ora": "4.0.3",
    "pg": "7.17.1",
    "shelljs": "0.8.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "4.2.4",
    "@types/jest": "24.0.25",
    "axios": "0.19.1",
    "boxen": "4.2.0",
    "chai": "4.2.0",
    "cpy-cli": "3.0.0",
    "cucumber": "6.0.5",
    "dumdum": "1.0.0-alpha.4",
    "imgur": "0.3.1",
    "puppeteer": "2.0.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
