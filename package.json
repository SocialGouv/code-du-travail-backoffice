{
  "name": "@socialgouv/code-du-travail-backoffice",
  "version": "0.0.0",
  "description": "Applications d'administration des données du code du travail numérique.",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build": "lerna run build",
    "ci:test:e2e": "NODE_ENV=test ./scripts/prod/start.sh && yarn test:e2e",
    "db:import": "node ./scripts/db/importGoogleFormData.js",
    "db:migrate": "knex migrate:latest",
    "db:migrate:make": "node ./scripts/db/generateMigration.js",
    "db:seed": "knex seed:run",
    "dev": "lerna run dev --parallel",
    "setup": "lerna bootstrap && node ./scripts/setup",
    "start": "lerna run start --parallel",
    "start:prod": "NODE_ENV=production ./scripts/prod/start.sh",
    "test": "lerna run test",
    "test:e2e": "cucumber-js",
    "test:unit": "lerna run test:unit",
    "test:update": "lerna run test:update",
    "test:watch": "lerna run test:watch --parallel"
  },
  "dependencies": {
    "boxen": "4.1.0",
    "chai": "4.2.0",
    "csv-parse": "4.4.3",
    "cucumber": "5.1.0",
    "dotenv": "8.0.0",
    "imgur": "0.3.1",
    "knex": "0.19.0",
    "lerna": "3.16.1",
    "moment": "2.24.0",
    "ora": "3.4.0",
    "pg": "7.11.0",
    "puppeteer": "1.18.1",
    "uuid": "3.3.2"
  }
}
